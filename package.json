{
  "name": "ts-template",
  "version": "1.0.0",
  "description": "一套简单的ts项目模板",
  "author": "CaoMeiYouRen",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "lint": "cross-env NODE_ENV=production eslint src --fix --ext .ts,.js",
    "build": "rimraf dist && tsc",
    "dev": "ts-node-dev src/index.ts",
    "rm": "rimraf node_modules",
    "watch": "tsc -w",
    "start": "npm run build && node ./dist/index"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/node": "^14.14.0",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "cross-env": "^7.0.2",
    "debug": "^4.2.0",
    "eslint": "^7.11.0",
    "eslint-config-cmyr": "^0.10.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {}
}
