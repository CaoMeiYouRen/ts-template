{
  "name": "ts-template",
  "version": "0.1.0",
  "description": "一套简单的ts项目模板",
  "author": "CaoMeiYouRen",
  "private": true,
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "lint": "cross-env NODE_ENV=production eslint src --fix --ext .ts,.js",
    "build": "rimraf dist && tsc",
    "dev": "cross-env NODE_ENV=development ts-node-dev src/index.ts",
    "rm": "rimraf node_modules",
    "watch": "tsc -w",
    "start": "node ./dist/index"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "cross-env": "^7.0.2",
    "debug": "^4.4.0",
    "eslint": "^8.57.1",
    "eslint-config-cmyr": "^1.1.32",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {}
}