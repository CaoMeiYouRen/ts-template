{
  "name": "ts-template",
  "version": "0.1.0",
  "description": "一套简单的ts项目模板",
  "author": "CaoMeiYouRen",
  "private": true,
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "lint": "cross-env NODE_ENV=production eslint src --fix --ext .ts,.js",
    "build": "rimraf dist && tsc",
    "dev": "cross-env NODE_ENV=development ts-node-dev src/index.ts",
    "rm": "rimraf node_modules",
    "watch": "tsc -w",
    "start": "node ./dist/index"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "cross-env": "^7.0.2",
    "debug": "^4.3.1",
    "eslint": "^7.21.0",
    "eslint-config-cmyr": "^1.0.10",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.2"
  },
  "dependencies": {}
}