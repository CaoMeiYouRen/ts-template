{
  "name": "ts-template",
  "version": "0.1.0",
  "description": "一套简单的ts项目模板",
  "author": "CaoMeiYouRen",
  "private": true,
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "lint": "cross-env NODE_ENV=production eslint src --fix --ext .ts,.js",
    "build": "rimraf dist && tsc",
    "dev": "cross-env NODE_ENV=development ts-node-dev src/index.ts",
    "rm": "rimraf node_modules",
    "watch": "tsc -w",
    "start": "node ./dist/index"
  },
  "devDependencies": {
    "@types/debug": "^4.1.6",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.1",
    "cross-env": "^7.0.2",
    "debug": "^4.3.2",
    "eslint": "^7.30.0",
    "eslint-config-cmyr": "^1.1.11",
    "rimraf": "^3.0.2",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  },
  "dependencies": {}
}